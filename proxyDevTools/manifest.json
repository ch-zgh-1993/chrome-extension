{
  "manifest_version": 3,
  "name": "proxy chrome devtools",
  "description": "js extension",
  "version": "1.0.0",
  "devtools_page": "devtools.html",
  "author": "zgh_3231@163.com",
  "icons": {
    "16": "./icon.png"
  },
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "action": {
    "default_popup": "./popup/index.html",
    "default_title": "proxy tools",
    "default_icon": {
      "16": "./icon.png"
    }
  }
}